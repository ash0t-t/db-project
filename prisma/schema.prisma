generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
}
model Enterprise {
  id             Int      @id @default(autoincrement())
  name           String
  activityType   String?
  employeesCount Int?
  supplies       Supply[]
}
model Product {
  id            Int      @id @default(autoincrement())
  shortName     String
  fullName      String?
  unit          String?
  purchasePrice Float?
  shelfLifeDays Int?
  supplies      Supply[]
}
model Supply {
  id         Int      @id @default(autoincrement())
  enterprise Enterprise @relation(fields: [enterpriseId], references: [id])
  enterpriseId Int
  product    Product   @relation(fields: [productId], references: [id])
  productId  Int
  date       DateTime
  volume     Float
  salePrice  Float?
}